<div class="container">
  <div class="card mb-3">
    <div class="row g-0">
      <div class="col-md-4">
        <img src="https://upload.wikimedia.org/wikipedia/commons/7/70/Dog_silhouette.svg" width="150px" height="150px" class="img-fluid rounded-start" alt="activity card photo">

      </div>
      <div class="col-md-8">
        <div class="card-body">
          <h5 class="card-title"><%= dog.id + 1 %>. <%= dog.name %></h5>
          <p class="card-text"><%= dog.name %></p>
          <p class="card-text"><%= dog.biography %></p>
        </div>
      </div>
    </div>
  </div>
</div>
