<div class="container">
  <div class="card mb-3">
    <div class="row g-0">
      <div class="col-md-4">
        <%= cl_image_tag(dog.cl_tag, width: 150, height: 150, class: "img-fluid rounded-start") %>
      </div>
      <div class="col-md-8">
        <div class="card-body">
          <h5 class="card-title"><%= dog.id + 1 %>. <%= dog.name %></h5>
          <p class="card-text"><%= dog.name %></p>
          <p class="card-text"><%= dog.biography %></p>
        </div>
      </div>
    </div>
  </div>
</div>
